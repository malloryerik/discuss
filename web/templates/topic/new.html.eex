<h5>Sticky's asleep </h5>

<%= form_for @changeset, topic_path(@conn, :create), fn f ->  %>
    <div class="form-group">
        <%= text_input f, :title, placeholder: "Title", class: "form-control" %>
         
        <%= error_tag f, :title %>
       
    </div>

    <%= submit "Save Topic", class: "btn btn-primary indigo accent-1" %>
<% end %>
